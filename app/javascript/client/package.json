    "postinstall": "expo-yarn-workspaces postinstall && yarn patch:reanimated",
    "android:rails": "adb reverse tcp:4001 tcp:4001 && adb reverse tcp:4000 tcp:4000",
    "android:react": "adb reverse tcp:8081 tcp:8081",
    "android:web": "adb reverse tcp:8080 tcp:8080 && adb reverse tcp:4333 tcp:4333",
    "android:crash": "adb logcat -b crash",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:logs": "adb logcat --pid=`adb shell pidof -s de.macbury.detox`",
    "android:cowgirl": "yarn android:rails && yarn android:react && yarn android:web",
