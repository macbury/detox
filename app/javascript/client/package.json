  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "node server.js",
    "web:build": "NODE_ENV=production next build",
    "web:start": "NODE_ENV=production node server.js",
    "web:test": "PROXY_KEY=0x24FEEDFACEDEADBEEFCAFE FRONTEND_PORT=5555 yarn web:start",
